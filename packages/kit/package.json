{
  "name": "@clayout/kit",
  "version": "0.0.1",
  "description": "Editor kit that provides block components and renderer functions",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build:js": "tsup src/index.ts --format cjs,esm",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json",
    "build": "pnpm build:js && pnpm build:types",
    "prepare": "pnpm build",
    "lint": "eslint .",
    "test": "echo \"No test specified\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@clayout/interface": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-sonarjs": "^3.0.4",
    "globals": "^16.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.37.0"
  }
}
