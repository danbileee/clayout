{
  "name": "@clayout/interface",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && node -e \"require('fs').mkdirSync('dist/esm',{recursive:true}); require('fs').writeFileSync('dist/esm/package.json', JSON.stringify({type:'module'}))\"",
    "prepare": "pnpm build",
    "test": "echo \"No test specified\""
  },
